<% provide(:title, "playlist") %>
<h1>Playlist Name</h1>
<div class="row">
 <div class="col-md-6 col-md-offset-3">
  <%= form_for :playlist do |f| %>
    <%= f.label :name %>
     <%= f.text_field :name, class: "form-control" %>
     <%= f.submit "Create my Playlist", class: "btn btn-primary" %>
    <% end %>
 </div>
</div>