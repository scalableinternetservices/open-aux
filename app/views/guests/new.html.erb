<div class="form-wrapper">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
    <%= form_with(url: "/linking", method: "post") do %>
      <%= label_tag(:userName, "name:") %>
        <%= text_field_tag(:userName)%>


        <%= label_tag(:key, "Room ID:") %>
        <%= text_field_tag(:key)%>

        <%= submit_tag("Join Room") %>
      <% end %>
    </div>
  </div>
</div>
